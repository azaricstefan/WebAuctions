@using IEP___projekat_AS.Models
@model IEnumerable<IEP___projekat_AS.Models.Auction>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Auctions Won";
}
<br /><br />
<table class="table">

    <tr>
        <th>
            @Html.DisplayNameFor(model => model.name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.price)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.creation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.opening)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.closing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.details)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.creation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.opening)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.closing)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.details)
            </td>
            <td>
                @Html.ActionLink("Details", "Details", new { id = item.Id })
            </td>
        </tr>
    }
</table>